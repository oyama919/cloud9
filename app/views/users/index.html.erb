<div class="container">
  <h2>ユーザー</h2>
  <% @users.each do |user| %>
      <div class="db">
        <p class="img"><%= profile_img(user) %><p>
        <%= user.email %>
        <% if user.id != current_user.id %>
          <%= render 'follow_form', user: user %>
        <% end %>
        <%= link_to user.name, user_path(user) %>
      </div>
      <br>
  <% end %>
</div>
